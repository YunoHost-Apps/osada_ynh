{
    "packaging_format": 1,
    "id": "osada",
    "name": "osada",
    "description": {
        "en": "Decentralized publication platform and social network",
        "fr": "Plateforme de publication décentralisée et un réseau social"
    },
    "url": "http://zotlabs.com/osada/",
    "license": "MIT",
    "version": "21.05.21~ynh1",
    "maintainer": {
        "name": "Anmol Sharma",
        "email": ""
    },
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql",
        "postgresql"
    ],
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": true,
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            },
            {
                "name": "database",
                "type": "string",
                "ask": {
                    "en": "Choose the database to be used for the Osada [mysql:1,postgresql:2]",
                    "fr": "Choisissez la database de Osada [mysql:1,postgresql:2]"
                },
                "choices": ["1", "2"],
                "default": "1"
            }
        ]
    }
}
